import java.util.Scanner;

class Main {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		while (scan.hasNext())
		{
			double n = scan.nextDouble();
			double p = scan.nextDouble();
			System.out.println(Math.round(Math.pow(p, 1/n)));
		}
		scan.close();
	}
}